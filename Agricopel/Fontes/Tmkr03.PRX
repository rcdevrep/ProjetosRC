			oPrn:Say( Li-7,040 , "Total das quantidades:" + Transform(nTotQtd, PESQPICT("SUB","UB_QUANT") ),oFont12,100)
			oPrn:Say( Li-7,044*nTamcol , "Valor total do Pedido:" + Transform((nTotGeral+xTotGeral), "@E 99,999,999.99"),oFont12,100)
			oPrn:Say( LI+43,040		, time() + " | Pagina: "+Alltrim(str(nPagA))+" - "+cPagR,oFont12,100)

		EndIf

		aFatura   := {}
		nTotQtd   := 0
		nTotGeral := 0
		xTotQtd   := 0
		xTotGeral := 0

		dbSelectArea("ALA")
		dbSkip()

	End

	//Imprime as Condições gerais
	LI+=nTamLinha
	oPrn:Box(LI,030,LI+8*nTamLinha,2355)
	LI+=nTamLinha
	oPrn:Say(Li, 1000, 'CONDIÇÕES GERAIS', oFont16)
	LI+=nTamLinha
	oPrn:Say(Li, 70, '- Orçamento válido por 24 horas.', oFont10)
	LI+=nTamLinha
	oPrn:Say(Li, 70, '- Preços validos conforme condições acima ou enquanto durarem os estoques.', oFont10)
	LI+=nTamLinha
	oPrn:Say(Li, 70, '- Orçamentos com condições a prazo estão sujeitos à análise e aprovação de crédito.', oFont10)
	LI+=nTamLinha
	oPrn:Say(Li, 70, '- Não aceitamos devoluções de mercadorias solicitadas sob encomenda.', oFont10)
	LI+=nTamLinha
	oPrn:Say(Li, 70, '- Devoluções somente com nossa autorização dentro do prazo máximo de 7 dias conforme CDC art. 49.', oFont10)
	LI+=nTamLinha
	oPrn:Say(Li, 70, '- Devoluções ocasionadas por erros do cliente, será cobrado custo de frete para retorno.', oFont10)

	oPrn:EndPage()

	//ÚÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄ¿
	//³Função de impressão do rodape na pagina final do relatorio³
	//ÀÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÙ

	If (Select("ALA") <> 0)
		dbSelectArea("ALA")
		dbCloseArea()
	Endif

	cPath := "C:\ORCAMENTO\"
	MakeDir(Trim(Upper(cPath)))

	oPrn:SetPortrait()
	oPrn:SetPaperSize(9)
	oPrn:lServer  := .F.
	oPrn:cPathPDF := cPath
	oPrn:SetViewPDF(.F.)
	oPrn:Preview()
	SetPgEject(.F.)
	MS_Flush()

	If _lemail
		Envmail(cpath,cFile)

		//Realiza a Exclusão dos arquivos
		// que foram gerados nos dias anteriores
		aLimpar := Directory( cpath + '*.*' )
		i := 1
		For i := 1 to len(aLimpar)
			if aLimpar[i][3] < ddatabase
				FErase( cpath + aLimpar[i][1] )
			endif
		Next i
	Endif
Return

