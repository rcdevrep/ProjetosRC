			nTotQtd  := 0
			nTotGeral:= 0

			cProduto := ''
			xTotQtd  := 0
			xTotGeral:= 0
			xVrunit  := 0

			nPagA := 1

			dbSelectArea("SUB")
			Do While !Eof() .and. (xFilial("SUB") == SUB->UB_FILIAL) .and. (ALA->UA_NUM    == SUB->UB_NUM)

				If Li == (90*nTamLinha)

