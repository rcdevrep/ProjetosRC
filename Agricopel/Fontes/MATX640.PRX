Static Function R640Impr(cAlias,cCliente,cLoja,cVend,aCoord,aTam)

	Local nValor := Iif(cAlias=="SE1",xMoeda(SE1->E1_SALDO,SE1->E1_MOEDA,MV_PAR13, SE1->E1_EMISSAO),(cAliasTmp)->TRB_VALOR)

	Local aSituacao :={"Carteira","Simples","Descontada","Caucionada",;	
	"Vinculada","Advogado","Judicial","Caucao Desc." }

	dbSelectArea("SA1")
	dbSeek(xFilial()+cCliente+cLoja)

	@li,aCoord[1]  Psay IIF(cAlias=="SE1",SE1->E1_PREFIXO,(cAliasTmp)->TRB_PREFIX)
	@li,aCoord[2]  Psay IIF(cAlias=="SE1",SE1->E1_NUM,(cAliasTmp)->TRB_NUM)
	@li,aCoord[3]  Psay IIF(cAlias=="SE1",SE1->E1_PARCELA,(cAliasTmp)->TRB_PARCEL)
	@li,aCoord[4]  Psay IIF(cAlias=="SE1",SE1->E1_CLIENTE,(cAliasTmp)->TRB_CLIE)
	@li,aCoord[5]  Psay IIF(cAlias=="SE1",SE1->E1_LOJA,(cAliasTmp)->TRB_LOJA)
	@li,aCoord[6]  Psay Left(SA1->A1_NOME, aTam[1])
	@li,aCoord[7]  Psay IIF(cAlias=="SE1",SE1->E1_EMISSAO,(cAliasTmp)->TRB_EMISSA)
	@li,aCoord[8]  Psay IIF(cAlias=="SE1",SE1->E1_VENCREA,(cAliasTmp)->TRB_VENCRE)
	@li,aCoord[9]  Psay nValor Picture tm(nValor,18)
	@li,aCoord[10] Psay IIF(cAlias=="SE1",SE1->E1_SUPERVI,(cAliasTmp)->TRB_SUPERV)
	@li,aCoord[11] Psay cVend
	@li,aCoord[12] Psay IIF(cAlias=="SE1",SE1->E1_PEDIDO,(cAliasTmp)->TRB_PEDIDO)
	@li,aCoord[13] Psay alltrim(SA1->A1_DDD)
	@li,aCoord[14] Psay alltrim(SA1->A1_TEL)
	@li,aCoord[15] Psay IIF(cAlias=="SE1",SE1->E1_TIPO,(cAliasTmp)->TRB_TIPO)
	If cAlias == "SE1"
		If Empty(SE1->E1_HIST) // Caso E1_HIST com conteudo (Ex.: PROTESTADO) apresentar este controle manual cfe Fernando/Financeiro 07/03/2008.
			@li,aCoord[16] Psay aSituacao[Val(SE1->E1_SITUACA)+1]
		Else
			@li,aCoord[16] PSAY Substr(SE1->E1_HIST,1,12)
		Endif
	Else
		If Empty((cAliasTmp)->TRB_HIST) // Caso E1_HIST com conteudo (Ex.: PROTESTADO) apresentar este controle manual cfe Fernando/Financeiro 07/03/2008.
			@li,aCoord[16] Psay aSituacao[Val((cAliasTmp)->TRB_SITUAC)+1]
		Else
			@li,aCoord[16] PSAY Substr((cAliasTmp)->TRB_HIST,1,12)
		Endif
	EndIf
	@li,aCoord[17] Psay alltrim(SA1->A1_CGC)

	dbSelectArea(cAlias)
Return

