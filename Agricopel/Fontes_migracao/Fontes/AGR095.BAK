	//12/07/2018 - Semaforo de Usuários
	FreeUsedCode()//Libera os Registro da SEE 		  
	dbselectarea('SEE')
	Reclock("SEE")
	  Replace EE_XUSER With ''
	SEE->(MsUnlock()) 
	             
	dbselectarea('SEE') 
	MsUnlockAll()

Return(.T.)



