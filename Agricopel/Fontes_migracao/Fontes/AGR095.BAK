	cQuery += " AND E1_PORTADO = '"+xBanco+"' "
	cQuery += " AND E1_AGEDEP = '"+xAgencia+"' "
	cQuery += " AND E1_CONTA  = '"+xConta+"' "  
	
	If Select("AGR095NN") <> 0
