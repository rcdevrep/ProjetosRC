/*/
Static Function cDigi(cVal)

	lBase  := Len(cVal)
	umdois := 2
	sumdig := 0
	auxi   := 0
	iDig := lBase
	Do While iDig >= 1
		auxi   := VAL(Subs(cVal,idig,1))*umdois
		sumdig := SumDig+IIF(auxi<10,auxi,INT(auxi/10)+auxi%10)
		umdois := 3-umdois
		iDig   := iDig-1
	Enddo
	auxi   := STR(Round(sumdig/10+0.49,0)*10-sumdig,1,0)

Return (cVal+auxi)       




