User Function AGR110()

	If INCLUI	
		If M->A1_BONIFIC == "S" .And. M->A1_TIPO <> "R"
			MsgStop("Cliente nao é Tipo Revendedor, nao podera receber Bonificacao!!!")
			Return .F.
		EndIf
	ElseIf ALTERA
		If M->A1_BONIFIC == "S" .And. SA1->A1_TIPO <> "R"
			MsgStop("Cliente nao é Tipo Revendedor, nao podera receber Bonificacao!!!")
			Return .F.
		EndIf	
	EndIf

Return .T.