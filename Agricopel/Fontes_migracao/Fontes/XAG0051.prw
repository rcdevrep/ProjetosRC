User Function XAG0051(cCnpj)

Local cQuery  := ""
Local cFilQry := ""

cQuery := ""
cQuery += "SELECT * "
cQuery += "FROM EMPRESAS "
cQuery += "WHERE EMP_CNPJ = '"+cCnpj+"' "

If Select("CNPJ") <> 0
	dbSelectArea("CNPJ")
	dbCloseArea()
Endif

TCQuery cQuery NEW ALIAS "CNPJ"

DbSelectArea("CNPJ")
DbGoTop()
While !Eof()
	
	cFilQry:= alltrim(CNPJ->EMP_FIL)
	
	DbSkip()
End

Return(cFilQry)