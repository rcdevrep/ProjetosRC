        EndIf
        If R75->(!dbSeek(cSeek))
			RecLock("R75",.T.)
			R75->A75_DTAINI	:= dDTIni
			R75->A75_DTAFIM	:= dDTFim
			R75->A75_PROD  	:= cProd
       	R75->A75_CODNCM	:= cNcm
