		A940LoadPro("I",@cNcm,@cProd)
   		
   		If Len(Alltrim(SB1->B1_COD))>14
            cSeek :=(dtos(dDtIni)+dtos(dDtFim)+PADR(cProd,14)+PADR(cNcm,8)+SB1->B1_UM+space(4))
