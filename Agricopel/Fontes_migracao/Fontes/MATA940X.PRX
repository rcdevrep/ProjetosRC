				A60->A60_NUMFAB	:=	(cAliasSFI)->FI_SERPDV
				A60->A60_SITTRI	:=	"CANC"
				A60->A60_VALOR	:=	(cAliasSFI)->FI_CANCEL
				MsUnlock()
			EndIf         
			If (cAliasSFI)->FI_NTRIB >0
				RecLock("A60",.T.)
				A60->A60_EMISS	:=	(cAliasSFI)->FI_DTMOVTO
				A60->A60_NUMFAB	:=	(cAliasSFI)->FI_SERPDV
				A60->A60_SITTRI	:=	"N"
				A60->A60_VALOR	:=	(cAliasSFI)->FI_NTRIB
				MsUnlock()
			EndIf    
			If (cAliasSFI)->FI_DESC >0
